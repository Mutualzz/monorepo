{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": ["dist/**", "build/**"]
        },
        "check-types": {
            "dependsOn": ["^check-types"],
            "outputs": []
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {
            "dependsOn": ["^lint"],
            "outputs": []
        }
    },
    "ui": "tui"
}
