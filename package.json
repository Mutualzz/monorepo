{
    "name": "mutualzz",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "check-types": "turbo run check-types",
        "start:rest": "pm2 start apps/rest/pm2.config.cjs --env production"
    },
    "devDependencies": {
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.1.0",
        "turbo": "^2.5.4",
        "typescript": "5.8.3"
    },
    "engines": {
        "node": ">=22"
    },
    "packageManager": "bun@1.2.18",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "trustedDependencies": [
        "@sentry/cli"
    ]
}
